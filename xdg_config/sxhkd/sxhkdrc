super + shift + BackSpace
    pkill -USR1 -x sxhkd


super + shift + q
    xkill


super + Return
    uxterm -e "tmux -f ~/.config/tmux/tmux.conf"

super + d
    rofi -show drun

super + w
    firefox --new-tab


